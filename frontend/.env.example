# Frontend .env.example
# This file shows example environment variables for the frontend service

# API URLs
NUXT_PUBLIC_API_BASE_URL=/api
NUXT_INTERNAL_API_BASE_URL=http://backend:5000
NUXT_PUBLIC_WS_URL=wss://dev.sobigidul.com

# Deployment settings
NITRO_HOST=0.0.0.0
NITRO_PORT=3000

# External services
MAPBOX_ACCESS_TOKEN=
NUXT_PUBLIC_MIDTRANS_CLIENT_KEY=
NUXT_PUBLIC_MIDTRANS_ENV=sandbox

# MikroTik Profile Names
# These must match the backend configuration in MIKROTIK_PROFILE_* vars
NUXT_PUBLIC_MIKROTIK_PROFILE_AKTIF=profile-aktif
NUXT_PUBLIC_MIKROTIK_PROFILE_UNLIMITED=profile-unlimited
NUXT_PUBLIC_MIKROTIK_PROFILE_INACTIVE=inactive
NUXT_PUBLIC_MIKROTIK_PROFILE_FUP=profile-fup
NUXT_PUBLIC_MIKROTIK_PROFILE_HABIS=profile-habis
NUXT_PUBLIC_MIKROTIK_PROFILE_BLOKIR=profile-blokir

# MikroTik URLs
NUXT_PUBLIC_MIKROTIK_LOGIN_URL=http://10.5.50.1/login

# Feature flags
NUXT_PUBLIC_ENABLE_IP_SOURCE_POLLING=true