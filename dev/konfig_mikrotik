/ip firewall filter
add action=drop chain=forward comment="STABIL: Drop QUIC for VIP Starlink (Fix GCloud/Speedtest)" disabled=yes dst-address-list=BEST_VIA_STARLINK \
    dst-port=443 protocol=udp
add action=accept chain=forward comment="ALLOW: Access to Server" dst-address=10.10.83.0/30
add action=accept chain=forward comment="ALLOW: Server Reply" src-address=10.10.83.0/30
add action=accept chain=forward comment="ALLOW: Server Reply" dst-address-list=whatsapp_otp_ips src-address=172.16.15.0/24
add action=drop chain=forward comment="STABIL: Drop Invalid Connections" connection-state=invalid
add action=accept chain=input comment="INPUT: Accept Established/Related" connection-state=established,related
add action=accept chain=forward comment="FORWARD: Accept Established/Related" connection-state=established,related
add action=accept chain=forward comment="ALLOW: Testing to 10.0.0.6" disabled=yes dst-address=10.0.0.6 src-address=172.16.15.0/24
add action=accept chain=forward comment="ALLOW: Testing to 10.0.0.6" disabled=yes dst-address=172.16.15.0/24 src-address=10.0.0.6
add action=accept chain=forward comment="Allow ADB Laptop to HP" dst-address=172.16.15.254 src-address=10.0.0.6
add action=accept chain=forward comment="Allow ADB HP to Laptop" dst-address=10.0.0.6 src-address=172.16.15.254
add action=drop chain=forward comment="STABIL: Cegah Leak LAN ke LAN" dst-address-type=!local out-interface-list=!LIST_WAN src-address=!10.0.0.0/29 \
    src-address-list=LOCAL_NETWORKS
add action=passthrough chain=unused-hs-chain comment="place hotspot rules here" disabled=yes
