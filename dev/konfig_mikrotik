add action=accept chain=input comment="INPUT: Allow Winbox from MGT" dst-port=8291 in-interface-list=MGT protocol=tcp
add action=accept chain=input comment="INPUT: Allow Ping from LANs/VLANs" in-interface-list=LIST_LAN protocol=icmp
add action=accept chain=input comment="INPUT: Allow API DOCKER" dst-port=8728,8729 in-interface-list=LIST_LAN protocol=tcp
add action=accept chain=input comment="INPUT: Allow DNS UDP from LANs/VLANs" dst-port=53 in-interface-list=LIST_LAN protocol=udp
add action=accept chain=input comment="INPUT: Allow DNS TCP from LANs/VLANs" dst-port=53 in-interface-list=LIST_LAN protocol=tcp
add action=accept chain=input comment="INPUT: Allow Traffic UDP MGT" in-interface-list=MGT protocol=udp
