add action=drop chain=input comment="INPUT: Drop All Other Input"
add action=drop chain=forward comment="FORWARD: Drop Non-MGT Access to Telkom Gateway GUI" dst-address=192.168.100.1 dst-port=80,443 in-interface-list=\
    !MGT protocol=tcp
add action=drop chain=forward comment="FORWARD: Drop Non-MGT Access to MGT Subnet" dst-address=10.1.1.0/24 in-interface-list=!MGT
add action=drop chain=forward comment="FORWARD: Drop Non-MGT Access to Server" dst-address=10.10.83.0/30 dst-port=22,23 in-interface-list=!MGT protocol=\
    tcp
add action=accept chain=forward comment="FORWARD: Allow NEW from LANs/VLANs to WAN" connection-state=new in-interface-list=LIST_LAN out-interface-list=\
    LIST_WAN
add action=accept chain=forward comment="FORWARD: Allow Inter-LAN/VLAN (UniFi etc)" in-interface-list=LIST_LAN out-interface-list=LIST_LAN
add action=accept chain=forward comment="FORWARD: Allow LAN to Zerotier" connection-state=new in-interface-list=LIST_LAN out-interface=zerotier1
add action=accept chain=forward comment="FORWARD: Allow Zerotier to ALL LAN" connection-state=new in-interface=zerotier1 out-interface-list=LIST_LAN
add action=drop chain=forward comment="FW-Fwd Final Drop:"
