# software id = 8DIE-0WA1
#
# model = RB1100x4
# serial number = HCJ08CSJQH6
/ip firewall nat
add action=accept chain=dstnat comment="VIP: User Game Direct" dst-address-list=BEST_VIA_TELKOM dst-port=53 protocol=udp
add action=accept chain=dstnat comment="VIP: User Game Direct" dst-address-list=BEST_VIA_TELKOM dst-port=53 protocol=tcp
add action=accept chain=dstnat comment="VIP: User Starlink Direct" dst-address-list=BEST_VIA_STARLINK dst-port=53 protocol=udp
add action=accept chain=dstnat comment="VIP: Server Direct" dst-port=53 protocol=udp src-address=10.10.83.0/30
add action=accept chain=dstnat comment="VIP: Server Direct" dst-port=53 protocol=tcp src-address=10.10.83.0/30
add action=accept chain=dstnat comment="VIP: Master Direct" dst-port=53 protocol=udp src-address=10.1.1.0/24
add action=accept chain=dstnat comment="VIP: Support" dst-port=53 protocol=udp src-address=10.0.0.0/29
add action=accept chain=dstnat comment="VIP: Support" dst-port=53 protocol=tcp src-address=10.0.0.0/29
add action=passthrough chain=unused-hs-chain comment="place hotspot rules here" disabled=yes
add action=masquerade chain=srcnat comment="NAT: Masquerade All WANs (Optimized & VPN Safe)" ipsec-policy=out,none out-interface-list=LIST_WAN
add action=masquerade chain=srcnat comment="Testing to 10.0.0.6 (Fix Firewall Target)" disabled=yes dst-address=10.0.0.6 ipsec-policy=out,none \
    src-address=172.16.15.0/24
add action=redirect chain=dstnat comment="STABIL: Force DNS UDP to Router" dst-address-type=!local dst-port=53 protocol=udp to-ports=53
add action=redirect chain=dstnat comment="STABIL: Force DNS TCP to Router" dst-address-type=!local dst-port=53 protocol=tcp to-ports=53
